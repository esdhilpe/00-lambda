
-------------------------------------------------------------------------------
-- DO NOT MODIFY THIS SEGMENT
-------------------------------------------------------------------------------

let TRUE  = \x y -> x
let FALSE = \x y -> y
let ITE   = \b x y -> b x y
let NOT   = \b x y -> b y x
let AND   = \b1 b2 -> ITE b1 b2 FALSE
let OR    = \b1 b2 -> ITE b1 TRUE b2

-------------------------------------------------------------------------------
-- YOU SHOULD ONLY MODIFY THE TEXT BELOW, JUST THE PARTS MARKED AS COMMENTS
-------------------------------------------------------------------------------

eval not_false:
  NOT FALSE
  =d> (\b x y -> b y x) FALSE
  =b> \x y -> FALSE y x
  =d> \x y -> (\x y -> y) y x
  =b> \x y -> (\y -> y) x
  =b> \x y -> x
  =d> TRUE

eval and_false_true:
  AND FALSE TRUE
  =d> (\b1 b2 -> ITE b1 b2 FALSE) FALSE TRUE
  =b> (\b2 -> ITE FALSE b2 FALSE) TRUE
  =b> ITE FALSE TRUE FALSE
  =d> (\b x y -> b x y) FALSE TRUE FALSE
  =b> (\x y -> FALSE x y) TRUE FALSE
  =b> (\y -> FALSE TRUE y) FALSE
  =b> FALSE TRUE FALSE
  =d> (\x y -> y) TRUE FALSE
  =b> \x -> TRUE FALSE
  =d> \x -> (\x y -> x) FALSE
  =b> \x y -> FALSE
  =d> \x y -> (\x y -> y)
  =b> \x y -> y
  =d> FALSE

eval or_true_false :
  OR TRUE FALSE
  =d> (\b1 b2 -> ITE b1 TRUE b2) TRUE FALSE
  =d> TRUE
